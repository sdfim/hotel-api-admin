{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "cheesegrits/filament-google-maps": "^3.0",
        "darkaonline/l5-swagger": "^8.5",
        "filament/actions": "^3.2",
        "filament/forms": "^3.2",
        "filament/tables": "^3.2",
        "filament/widgets": "^3.2",
        "google/apiclient": "^2.15.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^11.12",
        "laravel/jetstream": "^5.0",
        "laravel/octane": "^2.5",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.16",
        "livewire/livewire": "^3.3",
        "opcodesio/log-viewer": "^3.4",
        "opensearch-project/opensearch-php": "^2.2",
        "spatie/laravel-fractal": "^6.2",
        "spiral/roadrunner-cli": "^2.6",
        "spiral/roadrunner-http": "^3.5",
        "fakerphp/faker": "^1.23"
    },
    "require-dev": {
		"laravel/horizon": "^5.29",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/php-code-coverage": "^11.0.3",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Modules\\HotelContentRepository\\Models\\Factories\\": "Modules/HotelContentRepository/Models/Factories/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
